{% set links = content.field_social_media_links.0['#paragraph'].get('field_links').getValue() %}
<span class="inline-flex sm:ml-auto sm:mt-0 mt-4 justify-center sm:justify-start" >
  {% for link in links %}
    <a class="ml-3 {{ link.title }}" href="{{ link.uri }}" {{ attributes.setAttribute('target', '_blank') }}></a>
  {% endfor %}
</span>